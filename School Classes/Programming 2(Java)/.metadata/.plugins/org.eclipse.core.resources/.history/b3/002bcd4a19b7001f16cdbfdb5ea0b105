package lab2;

public abstract class GameCharacter implements Cloneable{
	protected String characterType;
	protected int healthPoints, attackPower, defensePower;
	
	abstract void play();
	
	public Object clone() {
		Object clone = null;
		
		try {
			clone = super.clone();
		}
		catch (CloneNotSupportedException e) {
			e.printStackTrace();
		}
		
		return clone;
	}

}
