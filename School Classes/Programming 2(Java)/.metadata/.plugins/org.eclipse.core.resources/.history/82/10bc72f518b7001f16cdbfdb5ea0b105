package lab2;

public class Warrior extends GameCharacter{
	
	//constructor
	public Warrior() {
		this.characterType = "Warrior";
		this.attackPower = 70;
		this.defensePower = 80;
		this.healthPoints = 100;
	}
	
	@Override
	void play() {
		System.out.println("The warrior does " + this.attackPower
				+ " damage upon attack.");
	}

}
